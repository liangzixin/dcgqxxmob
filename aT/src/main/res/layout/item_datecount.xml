<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:myApplication="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">

    <Space
        android:layout_width="match_parent"
        android:layout_height="6dp"/>

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        myApplication:cardCornerRadius="0dp"
        myApplication:cardElevation="4dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:padding="16dp">

            <TextView
                android:id="@+id/textTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="4dp"
                android:textColor="#111111"
                android:textSize="18sp"
                tools:text="Mock Title"
                android:gravity="center_vertical|center_horizontal"
                />
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                >
                <TextView
                    android:id="@+id/xinwen_duotu_date"
                    android:layout_width="wrap_content"
                    android:layout_marginRight="15dp"
                    android:textSize="15dp"
                    android:text=""
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/xinwen_duotu_org"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"

                    android:layout_height="wrap_content"
                    android:text="东川供求信息网"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_below="@+id/toutiaop_imgsrc"
                    android:layout_marginTop="5dp"
                    android:src="@mipmap/xinwen_toutiao_item"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />


            </LinearLayout>
            <TextView
                android:id="@+id/textContent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:lineSpacingMultiplier="1"
                android:padding="4dp"
                android:textColor="#000000"
                android:layout_marginTop="10dp"
                android:textSize="15sp"/>
            <!--访问统计-->
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content"
                android:id="@+id/category1"

                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"

                    >
                <TextView
                    android:id="@+id/count_countnume"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="15dp"
                    android:text="访问总次数："
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    />
                <TextView
                    android:id="@+id/count_countnume0"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:text="000000"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"

                >
                <TextView
                    android:id="@+id/count_current"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="15dp"
                    android:text="当前在线    ："
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    />
                <TextView
                    android:id="@+id/count_current0"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:text="当前在线    ："
                    />
        </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"

                    >
                <TextView
                    android:id="@+id/count_start"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="15dp"
                    android:text="开始日期  ："
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    />
                <TextView
                    android:id="@+id/count_start0"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:text="开始日期  ："
                    />
                    </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"

                    >
                <TextView
                    android:id="@+id/count_today"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="15dp"
                    android:text="今日访问量："
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    />
                <TextView
                    android:id="@+id/count_today0"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:text="今日访问量："
                    />
                    </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"

                    >
                <TextView
                    android:id="@+id/count_average"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="15dp"
                    android:text="平均访问量："
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    />
                <TextView
                    android:id="@+id/count_average0"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:text="平均访问量："
                    />
                    </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"

                    >
                <TextView
                    android:id="@+id/count_max"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="20dp"
                    android:textSize="15dp"
                    android:text="最高访问量："
                    android:layout_height="wrap_content"
                    android:textColor="#000000"
                    />
                <TextView
                    android:id="@+id/count_max0"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:textSize="15dp"
                    android:textColor="#000000"
                    android:layout_height="wrap_content"
                    android:text="最高访问量："
                    />
                    </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content"

                    >
                    <TextView
                        android:id="@+id/count_maxdate"
                        android:layout_width="wrap_content"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="20dp"
                        android:textSize="15dp"
                        android:text="最高发生日："
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        />
                    <TextView
                        android:id="@+id/count_maxdate0"
                        android:layout_width="wrap_content"
                        android:layout_marginTop="15dp"
                        android:layout_marginLeft="15dp"
                        android:textSize="15dp"
                        android:textColor="#000000"
                        android:layout_height="wrap_content"
                        android:text="最高发生日"
                        />
                </LinearLayout>
        </LinearLayout>

        </LinearLayout>
    </android.support.v7.widget.CardView>

    <Space
        android:layout_width="match_parent"
        android:layout_height="6dp"/>

</LinearLayout>